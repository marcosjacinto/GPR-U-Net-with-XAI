name: synthetic pipeline

entry_points:
  pipeline:
    parameters:
      hydra_options:
        description: Hydra parameters to override
        type: str
        default: ""
    command: "python process_data.py $(echo {hydra_options}) && \
      python segregate.py $(echo {hydra_options}) && \
      python transform.py $(echo {hydra_options}) && \
      python sample_data.py $(echo {hydra_options}) && \
      python augment.py $(echo {hydra_options}) && \
      python train.py $(echo {hydra_options})"
